{
  "name": "Chrome Extension Webpack",
  "options_page": "options.html",
  "background": {
    "page": "background.html"
  },
  "icons": {
    "128": "icon-128.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.bundle.js"]
    }
  ],
  "web_accessible_resources": ["worker.bundle.js"],
  "manifest_version": 2
}
